                <code value="Reference" />
                <targetProfile value="http://www.hl7.org/fhir/us/bser/StructureDefinition/PatientRole" />
            </type>
            <patternString value="BSeR Patient" />
        </element>
        <element id="MedicationAdministration.dosage">
