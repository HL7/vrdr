                    <reference value="http://www.hl7.org/fhir/ValueSet/medication-admin-status" />
                    <display value="Medication-admin-status" />
                </valueSetReference>
            </binding>
        </element>
        <element id="MedicationAdministration.medication[x]">
            <path value="MedicationAdministration.medication[x]" />
